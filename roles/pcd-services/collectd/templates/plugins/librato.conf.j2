## Librato Plugin

<Plugin "python">
    ModulePath "{{ librato_plugin_dir }}"
    Import "collectd-librato"

    <Module "collectd-librato">
        APIToken "{{ librato_apikey }}"
        Email "{{ librato_user }}"
        MetricPrefix ""
        FlushIntervalSecs {{ librato_interval }}
        LowercaseMetricNames true
        IncludeRegex "{{ librato_std_regex }}"
    </Module>
</Plugin>

